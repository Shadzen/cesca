                a.button._type-2._size-3(href='#')
                    span данные
                a.button._type-2._size-3(href='#')
                    span конференция
                a.button._type-2._size-3._active(href='#')
                    span встреча
                a.button._type-2._size-3(href='#')
                    span партнеры
        .separator._horizontal._small
        section.section-news._with-box._resizable-boxes
            .box._wrap
