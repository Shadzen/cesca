extends ../layout/layout.pug
block vars
    - var title = 'CESCA - Новости'
    - var bodyClass = "white"
block header
    - var name = "news"
    include ../includes/header.pug
    include ../includes/mixins.pug
block main
    main.news
        .page-head
            .page-head-background
                video(autoplay='' loop='' muted='' poster='./img/news/page-head.jpg')
                    source(src='./video/news/page-head.mp4' type='video/mp4')
        .separator._horizontal._small._transparent
        section.section-1._with-box
            .box
                .box-item._width50
                    h2 Новости
                    h2
                        span.logo-2 CESCA
        .separator._horizontal._medium._transparent
        section.section-2
            .box._wrap
                a.button._type-2._size-3._active(href='#')
